{
 "database": {
  "host": "<IP-OF-HOST>",
  "name": "rims",
  "password": "rims",
  "username": "rims"
 },
 "id": "master",
 "master": "http://<IP-OF-HOST>:8080",
 "startupdelay":20,
 "port": 8080,
 "debug": false,
 "ssl": {
  "port": 8081,
  "certfile": "/rims/rims.crt",
  "keyfile": "/rims/rims.key",
  "password": "<PASSWORD-IF-NEEDED>"
 },
 "influxdb": {
  "url":"http://<IP-OF-HOST>:8086",
  "org":"<ORG>",
  "bucket":"<BUCKET>",
  "token":"<TOKEN>"
 },
 "salt":"WBEUAHfO",
 "files": {
  "certs": "/rims/ssl"
 },
 "token": "ABCD1234",
 "template": "engine.init",
 "workers": 100,
 "logging": {
  "rest": {
   "enabled": true,
   "file": "/var/log/rims/rims.rest.log"
  },
  "system": {
   "enabled": true,
   "file": "/var/log/rims/rims.system.log"
  }
 },
 "snmp": {
  "read": "public",
  "write": "private",
  "timeout": "500000"
 },
 "netconf": {
  "password": "<PASSWORD>",
  "username": "root",
  "anonftp": "<IP-OF-HOST>",
  "dns": "<IP-OF-HOST>",
  "encrypted": "<ENCRYPTED PASSWORD>",
  "ntp": "<NTP SERVER>",
  "tacplus": "<TACPLUS SERVER>"
 },
 "tasks": [
  {
   "module":"ipam",
   "function":"check",
   "args":{ "repeat":300 }
  },
  {
   "module": "system",
   "function": "database_backup",
   "args": { "filename": "/var/log/rims/rims.backup" },
   "frequency": 86400
  },
  {
   "module":"services.hass",
   "function":"start"
  },
  {
   "module":"services.influxdb",
   "function":"process",
   "frequency":150
  }
 ],
 "services": {
  "nodns":{
   "file":"/var/log/rims/rims.nodns",
   "endpoint":"127.0.0.1:53"
  },
  "nibe":{
   "client_id":"<ID>",
   "client_secret":"<SECRET>",
   "redirect_uri":"http://<IP-OF-HOST>:8080/api/services/nibe/auth",
   "system_id":"SYSTEM_ID",
   "bucket":"<BUCKET>",
   "measurement":"nibe",
   "frequency":60,
   "state":"ABCD1234",
   "token_file":"/var/local/rims.nibe.token"
  },
  "oui": {
   "location": "http://standards-oui.ieee.org/oui.txt"
  },
  "iscdhcp": {
   "active": "/rims/dhcpd.leases",
   "reload": "/usr/bin/docker restart dhcpd",
   "static": "/rims/rest.conf"
  },
  "powerdns": {
   "recursor": {
    "url":"http://pdns-recursor-1:8082",
    "key":"ABCD1234"
   },
   "server": {
    "url":"http://pdns-authorative-1:8081",
    "key":"ABCD1234",
    "nameserver": "server.local.",
    "endpoint": "172.18.0.5:53"
   }
  }
 },
 "site":{
  "portal": {
   "message": "Management Portal Login",
   "title":"Management",
   "start": "devices",
   "theme": "light"
  },
  "menuitem": {
   "activities": {
    "module":  "activity",
    "function": "Main"
   },
   "devices": {
    "module": "device",
    "function": "Main"
   },
   "hypervisor": {
    "module": "hypervisor",
    "function": "Main"
   },
   "resources": {
    "module": "resource",
    "function": "Main"
   }
  },
  "resource": {
   "influxdb": {
    "tab": "http://<IP-OF-HOST>:8086"
   },
   "visualize": {
    "module": "visualize",
    "function": "Main"
   },
   "rack": {
    "module": "rack",
    "function": "Main"
   },
   "inventory": {
    "module": "inventory",
    "function": "Main"
   }
  }
 }
}
